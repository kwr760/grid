<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.roy.data.objects">
	<class name="Directory" table="`Directory`">
		<id column="`DirectoryID`" name="DirectoryID">
			<generator class="assigned" />
		</id>
		<property column="`DirectoryName`" generated="never"
			lazy="false" name="DirectoryName" type="string" />
		<property column="`FullPath`" generated="never"
			lazy="false" name="FullPath" type="string" />
	</class>
</hibernate-mapping>
