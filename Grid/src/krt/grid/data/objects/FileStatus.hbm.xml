<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.roy.data.process.objects">
	<class name="FileStatus" table="`FileStatus`">
		<id column="`FileStatusID`" name="FileStatusID">
			<generator class="assigned" />
		</id>
		<property column="`FileStatusName`" generated="never"
			lazy="false" name="FileStatusName" type="string" />
	</class>
</hibernate-mapping>
